{
    "inputs": [
        {
            "type": "promptString",
            "id": "n8n-mcp-token",
            "description": "Your n8n-MCP AUTH_TOKEN",
            "password": true
        }
    ],
    "servers": {
        "sequential-thinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ]
        },
        "interactive-mcp": {
            "url": "http://localhost:8090/mcp"
        },

        "chrome-devtools": {
            "command": "npx",
            "args": ["chrome-devtools-mcp@latest"]
        }
    }
}